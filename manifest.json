{
   "background": {
      "page": "background.html"
   },
   "content_scripts": [ {
      "css": [ "css/style.css" ],
      "js": [ "js/jquery.min.3.7.1.js", "js/main.js" ],
      "matches": [ "*://news.ycombinator.com/*" ],
      "run_at": "document_idle"
   } ],
   "content_security_policy": "default-src 'self'",
   "description": "A nice, readable, dark theme for Hacker News.",
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "48": "icons/48.png"
   },
   "manifest_version": 2,
   "name": "HackerNewsDark",
   "version": "0.9"
}
