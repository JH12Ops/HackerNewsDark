{
   "manifest_version": 3,
   "name": "HackerNewsDark",
   "description": "A nice, readable, dark theme for Hacker News.",
   "version": "0.11.1",
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "48": "icons/48.png"
   },
   "permissions": [],
   "host_permissions": [
      "*://news.ycombinator.com/*"
   ],
   "background": {
      "page": "background.html"
   },
   "content_scripts": [ {
      "css": [ "css/style.css" ],
      "js": [ "js/main.js" ],
      "matches": [ "*://news.ycombinator.com/*" ],
      "run_at": "document_idle"
   } ],
   "browser_specific_settings": {
      "gecko": {
         "id": "{647d2098-b33e-4f3b-80ed-76ec89fba5a2}",
         "strict_min_version": "58.0"
      }
   }
}
